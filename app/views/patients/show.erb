<h1>Patient: <%= @patient.name %></h1>
<ul>
  <% @patient.appointments.each do |appt| %>
    <li><h3><%= appt.appointment_datetime.strftime('%B %d, %Y') %> at
    <%= appt.appointment_datetime.strftime('%H:%M') %></h3>
    <h4><%= link_to appt.doctor.name, doctor_path(appt.doctor) %></h4>
  </li>

  <% end %>
</ul>
