<h1>Dr. <%= @doctor.name %>'s Appointments</h1>
<ul>
  <% @doctor.appointments.each do |appt| %>
    <li><h2><%= link_to appt.patient.name, patient_path(appt) %></h2></li>
    <%= appt.appointment_datetime.strftime('%B %d, %Y') %> at
    <%= appt.appointment_datetime.strftime('%H:%M') %>
  <% end %>
</ul>
